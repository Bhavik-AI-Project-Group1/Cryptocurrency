import pandas as pd

# Load the two datasets
file_path1 = "/content/new_lowercase_ADAUSD_1m_Coinbase.csv"
file_path2 = "/content/modified_deduped.csv"

df1 = pd.read_csv(file_path1)
df2 = pd.read_csv(file_path2)

# Concatenate the datasets
final_df = pd.concat([df1, df2], ignore_index=True)

# Save the concatenated dataset as finaldata.csv
final_file_path = "/content/finaldata.csv"
final_df.to_csv(final_file_path, index=False)

print(f"Both datasets have been concatenated and saved as: {final_file_path}")